{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"version": 1,
	"blocks": [
		{
			"type": "prompt",
			"alignment": "left",
			"segments": [
				{
					"type": "root",
					"style": "diamond",
					"leading_diamond": "\ue0b6",
					"trailing_diamond": "\ue0b0",
					"template": " \uf0e7 ",
					"background": "#BF616A",
					"foreground": "#ECEFF4",
					"Text": "",
					"Duration": 0,
					"NameLength": 0
				},
				{
					"type": "git",
					"style": "powerline",
					"powerline_symbol": "\ue0b0",
					"template": " {{ .User.Name }} ",
					"properties": {
						"fetch_user": true
					},
					"background": "#ECEFF4",
					"foreground": "#2E3440",
					"Text": "",
					"Duration": 0,
					"NameLength": 0
				},
				{
					"type": "path",
					"style": "powerline",
					"powerline_symbol": "\ue0b0",
					"template": " \uf07b .\\{{ .Folder }} ",
					"properties": {
						"folder_icon": "\uf115",
						"folder_separator_icon": " \ue0b1 ",
						"home_icon": "\uf7db",
						"style": "agnoster"
					},
					"background": "#177E89",
					"foreground": "#ECEFF4",
					"Text": "",
					"Duration": 0,
					"NameLength": 0
				},
				{
					"type": "git",
					"style": "powerline",
					"powerline_symbol": "\ue0b0",
					"background_templates": [
						"{{ if or (.Working.Changed) (.Staging.Changed) }}#28112B{{ end }}",
						"{{ if and (gt .Ahead 0) (gt .Behind 0) }}#B75A3C{{ end }}",
						"{{ if gt .Ahead 0 }}#88C0D0{{ end }}",
						"{{ if gt .Behind 0 }}#E3B7AA{{ end }}"
					],
					"template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }} ",
					"properties": {
						"branch_icon": "\ue725 ",
						"fetch_status": true,
						"fetch_upstream_icon": true
					},
					"background": "#28112B",
					"foreground": "#ECEFF4",
					"Text": "",
					"Duration": 0,
					"NameLength": 0
				}
			]
		},
		{
			"type": "prompt",
			"alignment": "right",
			"segments": [
				{
					"type": "sysinfo",
					"style": "powerline",
					"powerline_symbol": " \ue0b2",
					"invert_powerline": true,
					"template": " \ue266 {{ (div ((sub .PhysicalTotalMemory .PhysicalAvailableMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB ",
					"background": "#ac1db9",
					"foreground": "#ECEFF4",
					"Text": "",
					"Duration": 0,
					"NameLength": 0
				},
				{
					"type": "spotify",
					"style": "powerline",
					"powerline_symbol": "\ue0b2",
					"invert_powerline": true,
					"template": " \uf1bc {{ if ne .Status \"stopped\" }}{{ .Artist }} - {{ .Track }}{{ end }} ",
					"background": "#1DB954",
					"foreground": "#ECEFF4",
					"Text": "",
					"Duration": 0,
					"NameLength": 0
				},
				{
					"type": "time",
					"style": "diamond",
					"invert_powerline": true,
					"leading_diamond": "\ue0b2",
					"trailing_diamond": "\ue0b4",
					"template": " {{ .CurrentDate | date .Format }}",
					"properties": {
						"time_format": "15:04"
					},
					"background": "#272838",
					"foreground": "#ECEFF4",
					"Text": "",
					"Duration": 0,
					"NameLength": 0
				}
			]
		},
		{
			"type": "prompt",
			"alignment": "left",
			"segments": [
				{
					"type": "text",
					"style": "plain",
					"template": "‚ùØ ",
					"background": "",
					"foreground": "",
					"Text": "",
					"Duration": 0,
					"NameLength": 0
				}
			],
			"newline": true
		}
	]
}